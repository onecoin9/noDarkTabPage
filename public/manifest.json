{
    "manifest_version": 3,
    "name": "自定义新标签页",
    "version": "2.0.0",
    "description": "一个现代化的自定义新标签页，支持书签管理、Custom CSS 和数据同步",
    "author": "Constantine",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "自定义新标签页"
    }
}